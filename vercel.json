{
  "version": 2,
  "name": "lavalink-proxy",
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "env": {
    "LAVALINK_HOST": "@lavalink_host",
    "LAVALINK_PASSWORD": "@lavalink_password"
  },
  "functions": {
    "index.js": {
      "maxDuration": 30
    }
  }
}